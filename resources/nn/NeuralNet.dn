const int RELU = 0

const int BACK_PROP = 0

uses gpu.LogicalComputeDevice

interface NeuralNet {
    NeuralNet(store LogicalComputeDevice dev)
    void setInputVectorSize(int sz)
    void setTrainingSetSize(int sz)
    void setOutputVectorSize(int sz)
    void setHiddenLayerSizes(int sz[])
    void setHiddenLayerActivation(int layer, int function)
    void setTrainingAlgo(int algo)
    void train(dec trainSet[][])
    dec[] predict(dec vect[])
}
